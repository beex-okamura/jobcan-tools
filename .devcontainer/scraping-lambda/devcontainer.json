{
    "name": "jobcan-scraping-lambda",

    // "dockerFile": "../../functions/scraping-lambda/Dockerfile.local",
    "dockerComposeFile": ["../../functions/scraping-lambda/docker-compose.yml"],
    "service": "scraping-lambda",
    "workspaceFolder": "/usr/src/app/functions/scraping-lambda/",
    // Uncomment the next line if you want start specific services in your Docker Compose config.
    "customizations": {
      "vscode": {
        "settings": {
          "editor.codeActionsOnSave": {
            "source.fixAll": "always"
          }
        },
        "extensions": [
          "editorconfig.editorconfig",
          "dbaeumer.vscode-eslint",
          "streetsidesoftware.code-spell-checker",
          "me-dutour-mathieu.vscode-github-actions",
          "github.copilot",
          "rafwilinski.serverless-vscode-snippets"
        ]
      }
    },
    // Uncomment the next line if you want to keep your containers running after VS Code shuts down.
    // "shutdownAction": "none",
